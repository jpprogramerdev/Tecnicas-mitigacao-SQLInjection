@model Usuario

@{
    Layout = "_Layout";

    ViewData["Title"] = "Login ";
}
<form asp-controller="Login" asp-action="Entrar" method="POST">
    <div class="form-group">
        <label for="cpf_user">CPF:</label>
        <input type="text" class="form-control" asp-for="@Model.Cpf" id="cpf_user" />
    </div>

    <div class="form-group">
        <label for="cpf_user">Senha:</label>
        <input type="password" class="form-control" asp-for="@Model.Senha" id="cpf_user" />
    </div>

    <div class="form-group">
        <button type="submit" class="btn btn-primary">Entrar</button>
    </div>
</form>
